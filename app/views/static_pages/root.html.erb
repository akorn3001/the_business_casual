<script id="bootstrap-current-user" type="text/javascript">
  window.staticImages = {
    magnifyingGlass: "<%= asset_path("magnifying-glass.png") %>"
  }
  // BOOTSTRAPPING THE CURRENT USER
  <% if current_user %>
    window.currentUser = <%= render(
      'api/users/user.json.jbuilder',
      user: current_user
    ).html_safe %>
  <% end %>
</script>

<main id="root">I AM ROOT</main>
